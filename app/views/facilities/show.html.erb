<p id="notice"><%= notice %></p>

<table>
  <% Facility::TYPES.each do |type| %>
    <tr>
      <td><%= type.capitalize %></td>
      <td><%= @facility[type] %></td>
      <td><%= link_to 'Upgrade', upgrade_franchise_facility_path(upgrading: type), method: :post %></td>
    </tr>
  <% end %>
</table>
